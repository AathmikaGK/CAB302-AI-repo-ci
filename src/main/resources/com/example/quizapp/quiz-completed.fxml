<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.quizapp.Controllers.QuizCompletedController">
    <children>
        <Label text="Quiz Completed!" layoutX="100" layoutY="50" style="-fx-font-size: 24;"/>
        <Button fx:id="finishButton" text="Finish" layoutX="100" layoutY="100" onAction="#handleFinish"/>
        <Button fx:id="retakeButton" text="Retake" layoutX="100" layoutY="150" onAction="#handleRetakeQuiz"/>
    </children>
</AnchorPane>